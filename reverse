import java.util.*;
import java.io.*;

public class Reversestr {

 public static void main(String args[])
 {
  System.out.println("enter the string to be reversed");
  Scanner s=new Scanner(System.in);
  String s1=s.next();
  for(int i=s1.length()-1;i>=0;i--)
  {
 	System.out.print(s1.charAt(i));
  }
 }
}
